{
  "build": {
    "command": "npm run build",
    "publish": ".next",
    "environment": {
      "NETLIFY_NEXT_PLUGIN_SKIP": "false",
      "NODE_VERSION": "18",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "plugins": [
    {
      "package": "@netlify/plugin-nextjs"
    }
  ],
  "functions": {
    "directory": "netlify/functions"
  }
}